<table class="booking-detail">
    <tr>
        <td>
            <div class="container-overview-image">
                <img src="{{ book.getPicture().getWebPath | apply_filter('search_list') }}" />
            </div>
            <div class="container-overview-description">
                <h2>{{ provider.name }}</h2>
                <p>{{ provider.fulladdress }}</p>
            </div>
            
            <div class="container-overview-stay">
                <p><b>Data</b> <br />
                    {{ dateTitle }}
                </p>
                {% for k, item in book.getAllItem %}
                    <p><b>Quadra </b> <br />
                       {{ book.getItemName(k) }}
                    </p>
                {% endfor %}
            </div>
        </td>
    </tr>
</table>
<div class="overview-price">
    <table class="booking-price">
        <tbody>
            <tr>
                <td>Pre√ßo total</td>
                <td align="right">R$ {{ book.price }}</td>
            </tr>
        </tbody>
    </table>
</div>